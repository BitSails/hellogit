#include<iostream>
#include<string>
using namespace std;

class food{
  private:
    int num-of_dishes;
    string dish_type;

    void set_type_name(string dish)
     {
         dish_type=dish;
     }
     
    void set_dish_num(int num)
     {
         dnum_of_dishes=num;
     }
   
    string get_type_name()
     {
         return dish_type;
     }

   int get_dish_num()
     {
         return num-of_dishes;
     }
};//class

